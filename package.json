{
  "name": "dott",
  "version": "1.0.0",
  "description": "Dott Interview Project",
  "main": "index.js",
  "keywords": [
    "typescript",
    "nodejs",
    "bitmap challenge",
    "bitmap",
    "challenge"
  ],
  "scripts": {
    "debug": "node -r ts-node/register --inspect src/index.ts",
    "lint": "eslint . --ext .ts",
    "start": "node out/index.js",
    "clean:build:start": "npm run clean:build && npm run start",
    "build": "tsc",
    "clean:build": "rimraf ./out && tsc",
    "start:dev": "nodemon",
    "test": "jest",
    "lint-fix": "eslint . --ext .ts --fix",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vigsank/dott.git"
  },
  "author": "vigsank",
